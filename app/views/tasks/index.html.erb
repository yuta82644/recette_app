<h1>タスク一覧</h1>
<%= render 'shared/room-sidebar' %>
<div class="footer_fixed">

<ul class= "wrapper">
  <% @tasks.each do |task| %>
    <li>
      <h3><%= task.title %></h3>
      <p><%= task.content %></p>
      <p><%= task.start_time %></p>
    </li>
  <% end %>
</ul>
<%= link_to 'room', room_path(@room) %>
</div>